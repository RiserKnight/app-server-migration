			"complexity": "major",
			"rule_type": "package",
			"remove": {
				"import": [
					"com.tangosol",
					"com.oracle.coherence"
				]
			},
			"recommendation": 2
		},{
			"id": 2,
			"name": "WebLogic FTP Adapter",
			"description": "Remove WebLogic FTP adapter",
			"complexity": "major",
			"rule_type": "package",
			"remove": {
				"import": [
					"oracle.tip.adapter.ftp"
				]
			},
			"recommendation": 3
		},{
			"id": 3,
			"name": "EJB",
			"description": "EJB",
			"complexity": "critical",
			"rule_type": "package",
			"remove": {
