{
	"analyzer": "com.amazon.aws.am2.appmig.estimate.java.JavaFileAnalyzer",
	"file_type": "java",
	"rules": [
		{
			"id": 1,
			"name": "OracleDriver",
			"description": "Oracle database drivers",
			"complexity": "minor",
			"mhrs": 1,
			"rule_type": "package",
			"remove": {
				"import": ["java.sql.DriverManager","oracle.jdbc.driver.OracleDriver"]
			},
			"recommendation": 1
		},
		{
			"id": 2,
			"name": "DBSpecific",
			"description": "DB Specific SQL",
			"complexity": "major",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["java.sql.PreparedStatement"]
			},
			"recommendation": 2
		},
		{
			"id": 3,
			"name": "DBSpecific",
			"description": "DB Specific SQL",
			"complexity": "major",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["java.sql.CallableStatement"]
			},
			"recommendation": 3
		},		
		{
			"id": 4,
			"name": "DBSpecific",
			"description": "Oracle types",
			"complexity": "major",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["oracle.jdbc.OracleTypes"]
			},
			"recommendation": 4
		},
		{
			"id": 5,
			"name": "DBSpecific",
			"description": "Advanced Queuing features of Oracle for enterprise messaging applications",
			"complexity": "critical",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["oracle.jdbc.aq"]
			},
			"recommendation": 5
		},
		{
			"id": 6,
			"name": "DBSpecific",
			"description": "Oracle JDBC extension that provides interfaces to access the Database Change Notification feature of Oracle.",
			"complexity": "critical",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["oracle.jdbc.dcn"]
			},
			"recommendation": 6
		},
		{
			"id": 7,
			"name": "DBSpecific",
			"description": "Support for Distributed transactions",
			"complexity": "critical",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["oracle.jdbc.xa","oracle.jdbc.xa.client"]
			},
			"recommendation": 7
		},
		{
			"id": 8,
			"name": "DBSpecific",
			"description": "Oracle specific implementation of Connection cache and connection pooling",
			"complexity": "critical",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["oracle.jdbc.pool"]
			},
			"recommendation": 8
		},
		{
			"id": 9,
			"name": "DBSpecific",
			"description": "Oracle database transparent fail-over replay support",
			"complexity": "critical",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["oracle.jdbc.replay"]
			},
			"recommendation": 9
		}
	]
}
