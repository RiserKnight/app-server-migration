{
	"analyzer": "com.amazon.aws.am2.appmig.estimate.java.JavaFileAnalyzer",
	"file_type": "java",
	"rules": [
		{
			"id": 1,
			"name": "OracleDriver",
			"description": "Oracle database drivers",
			"complexity": "major",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["java.sql.DriverManager","oracle.jdbc.driver.OracleDriver"]
			},
			"recommendation": 1
		},
		{
			"id": 2,
			"name": "DBSpecific",
			"description": "DB Specific SQL",
			"complexity": "major",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["java.sql.Statement"]
			},
			"recommendation": 2
		},
		{
			"id": 3,
			"name": "DBSpecific",
			"description": "DB Specific SQL",
			"complexity": "major",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["java.sql.CallableStatement"]
			},
			"recommendation": 3
		},		
		{
			"id": 4,
			"name": "DBSpecific",
			"description": "Oracle types",
			"complexity": "minor",
			"mhrs": 2,
			"rule_type": "package",
			"remove": {
				"import": ["oracle.jdbc.OracleTypes"]
			},
			"recommendation": 4
		}
	]
}